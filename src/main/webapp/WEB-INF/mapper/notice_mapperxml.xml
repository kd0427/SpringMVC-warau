<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="notice">
	<select id="getNoticeList" resultType="com.global.vo.NoticeVO">
		<![CDATA[
			select * from notice_table order by notice_idx desc
		]]>
	</select>
	
	<select id="getNotice" resultType="com.global.vo.NoticeVO">
		<![CDATA[]
		
			selet * from notice_table where NOtice_idx
		
		]]>
	</select>
</mapper>